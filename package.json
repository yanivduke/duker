{
  "name": "duker",
  "version": "0.3.0",
  "description": "Terminal-based AI coding assistant with smart iteration and Vue3 UI",
  "main": "dist/index.js",
  "bin": {
    "duker": "./dist/cli-v3.js"
  },
  "type": "module",
  "scripts": {
    "dev": "tsx src/cli-v3.tsx",
    "dev:v1": "tsx src/cli.ts",
    "dev:v2": "tsx src/cli-v2.ts",
    "dev:v3": "tsx src/cli-v3.tsx",
    "build": "tsc",
    "start": "node dist/cli-v3.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai",
    "coding-assistant",
    "terminal",
    "agentic",
    "llm"
  ],
  "author": "DukeCode",
  "license": "MIT",
  "dependencies": {
    "ai": "^5.0.59",
    "@ai-sdk/anthropic": "^2.0.23",
    "@ai-sdk/openai": "^2.0.42",
    "@ai-sdk/google": "^2.0.17",
    "react": "^18.2.0",
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "boxen": "^7.1.1",
    "commander": "^12.0.0",
    "zod": "^3.22.4",
    "dotenv": "^16.4.5",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.1",
    "vitest": "^1.3.1",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
